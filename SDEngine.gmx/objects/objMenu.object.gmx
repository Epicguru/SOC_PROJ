        audio_play_sound(sndselect2,0,0)
    } else if keyboard_check_pressed(vk_up) {
        menuSelected -=1;
        if (menuSelected &lt; 1) { menuSelected = menuText[menuPage,0]; }
        audio_play_sound(sndselect2,0,0)
    } else if keyboard_check_pressed(global.kb_choose) {
        audio_play_sound(sndchoose,0,0)
