            with instance_create(0,0,objFadeOut) {
                effect_leave = tr_pixel;       
                effect_enter = tr_pixel;
                fadespd = 1/25
            }
            ok = 2
        } else if (menuSelected = 2) {   
            global.theroom = rmControls
            instance_create(0,0,objFadeOut)
            ok = 2
        } else if (menuSelected = 3) {  
            game_end() 
        }
    } 
}
