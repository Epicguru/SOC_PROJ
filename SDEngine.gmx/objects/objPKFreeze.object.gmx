                    b.str[1] = "The psychic power shield deflected the attack!"
                }
            }
        
            if dmg &gt;= global.s_hp[tgt] {
                instance_create(0,0,objMortalDamage)
                msg = string(dmg)+" HP of mortal damage#   to "+global.s_name[tgt] + "!"
            } else {
